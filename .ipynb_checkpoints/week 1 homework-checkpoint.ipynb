    "import random\n",
    "print(\"歡迎使用晚餐小幫手\")\n",
    "print(\"請輸入你想吃的晚餐選項\")\n",
    "print(\"輸入完後請輸入0\")\n",
    "L = []\n",
    "while  True:\n",
    "    a = input(\":\" )\n",
    "    if a == \"0\":\n",
    "        break\n",
    "    else:\n",
    "        L.append(a)\n",
    "random.shuffle(L)\n",
    "print(f'你今天吃的是 {L.pop(0)}' )\n",
    "input(\"按下enter 結束程式\")"
   ]
